<p id="notice"><%= notice %></p>

<h2 class="mou_list_title"><%= @mou_list.title %></h2>
<p class="mou_list_sub_title"><%= @mou_list.description %></p>

<div id="mou_items_wrapper">
  <%= render @mou_list.mou_items %>
  <div id="form">
    <%= render "mou_items/form" %>
  </div>
</div>

<div class="links">
  <%= link_to '修改', edit_mou_list_path(@mou_list) %> |
  <%= link_to "刪除", mou_list_path(@mou_list), method: :delete, data: { confirm: "Are you sure ?" } %> 
  <br>
  <%= link_to '返回', mou_lists_path %>
</div>
